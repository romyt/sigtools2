{% extends '::base.html.twig' %}

{% block title %}Creation d'un nouveau stock{% endblock %}

{% block body %}

<h1>Creation d'un nouveau stock</h1>

<form action="{{ path('TohouriStoreBundle_stock_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('TohouriStoreBundle_stock') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}