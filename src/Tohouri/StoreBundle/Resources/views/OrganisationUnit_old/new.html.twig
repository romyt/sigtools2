{% extends "TohouriStoreBundle::layout.html.twig" %}

{% block title %}OrganisationUnit list{% endblock %}

{% block body %}

<h1>OrganisationUnit creation</h1>

<form action="{{ path('TohouriStoreBundle_orgunit_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('TohouriStoreBundle_orgunit') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}